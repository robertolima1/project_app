<ScreenTecnicaView>:
  name: "screen_tecnica"
  md_bg_color: "#67AB9F"
  FloatLayout:    

    Image:
      source: 'images/background_0-0.png'
      size_hint: (1, 1)
      # size: "200dp", "200dp"
      pos_hint: {"center_x": 0.08, "center_y": 0.08}    

    Image:
      source: 'images/background_1-0.png'
      size_hint: (1, 1)
      # size: "200dp", "200dp"
      pos_hint: {"center_x": 0.92, "center_y": 0.08}    
    Image:
      source: 'images/background_0-1.png'
      size_hint: (1, 1)
      # size: "200dp", "200dp"
      pos_hint: {"center_x": 0.08, "center_y": 0.92}                
    Image:
      source: 'images/background_1-1.png'
      size_hint: (1, 1)
      # size: "200dp", "200dp"
      pos_hint: {"center_x": 0.92, "center_y": 0.92}          
      
    ScrollView:      
      MDList:
        id: list_tecnica
        pos_hint: {"center_x": 0.5, "center_y": 0.5}